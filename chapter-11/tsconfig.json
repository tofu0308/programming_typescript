{
  "compilerOptions": {
    "lib": ["es2015", "es2016.Array.Include", "dom"],
    "allowJs": false,
    "checkJs": false,
    "noImplicitAny": false,
    "strict": true,
    
    "module": "commonjs",
    "outDir": "dist",
    "sourceMap": true,
    "target": "es2020",
    "experimentalDecorators": true,
    "noImplicitReturns": true,
    "baseUrl": "./",
    "paths": {"base64-js": ["types/base64-js"]},
    "typeRoots": ["types", "node_modules/@types"],

  },
  "include": [
    "src"
  ]
}
